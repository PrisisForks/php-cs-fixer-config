{
    "name"       : "narrowspark/php-cs-fixer-config",
    "description": "Provides a configuration for fabpot/php-cs-fixer, used within Narrowspark.",
    "type"       : "library",
    "keywords"   : [
        "narrowspark",
        "cs",
        "cs-fixer",
        "config",
        "php-cs-fixer"
    ],
    "license"    : "MIT",
    "homepage"   : "https://github.com/narrowspark/php-cs-fixer-config",
    "support"    : {
        "issues": "https://github.com/narrowspark/php-cs-fixer-config/issues",
        "source": "https://github.com/narrowspark/php-cs-fixer-config"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                : ">=5.6",
        "fabpot/php-cs-fixer"                : "2.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit"                    : "~5.1.3",
        "satooshi/php-coveralls"             : "~0.7.1"
    },
    "autoload": {
        "psr-4": {
            "Narrowspark\\CS\\Config\\"      : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Narrowspark\\CS\\Config\\Test\\": "test"
        }
    },
    "minimum-stability"                      : "dev",
    "prefer-stable"                          : true
}
